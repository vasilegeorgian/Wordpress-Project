{
    "name": "devowl-wp/fast-html-tag",
    "description": "Find tags within HTML content and modify it in speed of light",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "config": {
        "sort-packages": true,
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "metasyntactical/composer-plugin-license-check": true
        }
    },
    "archive": {
        "exclude": [
            "lib",
            "tests"
        ]
    },
    "authors": [
        {
            "name": "DevOwl"
        }
    ],
    "extra": {
        "clean-all-except": [
            "src",
            "dist",
            "dev",
            "languages"
        ],
        "metasyntactical/composer-plugin-license-check": {
            "whitelist": [
                "MIT",
                "ISC",
                "BSD-2-Clause",
                "BSD-3-Clause",
                "BSD",
                "Apache-2.0",
                "Apache2",
                "Artistic-2.0",
                "WTFPL",
                "CC-0",
                "CC0-1.0",
                "MPL-2.0",
                "ZLib",
                "Unlicense",
                "GPL-2.0",
                "GPL-2.0-or-later",
                "GPL-3",
                "GPL-3.0-or-later",
                "LGPL-3.0-or-later"
            ],
            "blacklist": [
                "*"
            ],
            "packages": [
                "ignore-packages-here@1.0.0"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "DevOwl\\FastHtmlTag\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevOwl\\FastHtmlTag\\Test\\": "test/phpunit/"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "devowl-wp/composer-licenses": "@dev",
        "devowl-wp/phpcs-config": "@dev",
        "devowl-wp/phpunit-config": "@dev"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../*packages/*",
            "options": {
                "symlink": true
            }
        }
    ]
}
